 let number = 103
   let quant = 2

   switch (number) {
       case 100:
           let cQuente = quant * 1.70
           document.write("<br>O preço  do Cachorro Quente simples irá ser " + cQuente + " R$")
           break;
       case 101:
           let bSimples = quant * 2.30
           document.write("<br>O preço do Bauru Simples irá ser " + bauSimples + " R$")
           break;

       case 102:
           let bComOvo = quant * 2.60
           document.write("<br>O preço do Bauru Com Ovo irá ser " + bauComOvo + " R$")
           break;

       case 103:
           let hamb = quant * 2.40
           document.write("<br>O preço do Hamburguer irá ser " + hamb + " R$")
           break;

       case 104:
           let cheeseb = quant * 2.50
           document.write("<br>O preço do Cheeseburguer irá ser " + cheeseb + " R$")
           break;

       case 105:
           let refri = quant * 1.00
           document.write("<br>O preço do Refrigerante irá ser " + refri + " R$")
           break;
