document.write("Crescente: ");
     for (let number = 1; number <= 10; number++) {
         document.write(number +", ");
         

     }
     document.write("<br>");
     document.write("Decrescente: ");
     for (let number2 = 10; number2 >= 1; number2--) {
         document.write(number2 + ", ")
     }

